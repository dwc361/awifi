		resultClass="com.zjhcsoft.uac.authorization.resource.entity.System">
		<![CDATA[ 
		from System WHERE 1=1
		]]>
		<isNotEmpty prepend="and" property="app.id"> 
		<![CDATA[ 
		app.id = #app.id#
		]]> 
